    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.9",
    "@cloudflare/vitest-pool-workers": "^0.8.19",
    "@cloudflare/workers-types": "^4.20250423.0",
    "@effect/vitest": "0.20.13",
